{
  "model": {
    "name": "Xiao_Net",
    "window_len": 1000,
    "in_channels": 3,
    "num_phases": 3,
    "base_channels": 16
  },
  "training": {
    "batch_size": 32,
    "learning_rate": 0.001,
    "num_epochs": 100,
    "early_stopping_patience": 10,
    "distillation_alpha": 0.5,
    "temperature": 4.0
  },
  "data": {
    "data_path": "data/",
    "train_split": 0.8,
    "val_split": 0.1,
    "test_split": 0.1,
    "augment": true
  },
  "paths": {
    "checkpoint_dir": "checkpoints/",
    "log_dir": "logs/",
    "output_dir": "outputs/"
  },
  "device": "cuda",
  "seed": 42
}
